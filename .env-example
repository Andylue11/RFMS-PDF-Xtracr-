# RFMS PDF XTRACR - Environment Variables Example
# Copy this file to .env and fill in with your values

# Flask configuration
SECRET_KEY=your_secret_key_for_flask
DEBUG=True
PORT=5000

# Database configuration
DATABASE_URI=sqlite:///rfms_xtracr.db

# RFMS API configuration
RFMS_BASE_URL=https://api.rfms.online
RFMS_STORE_CODE=your_store_code
PASSWORD=RFMS_API_KEY
RFMS_API_KEY=your_api_key

# File upload configuration
UPLOAD_FOLDER=uploads
MAX_CONTENT_LENGTH=16777216  # 16MB 